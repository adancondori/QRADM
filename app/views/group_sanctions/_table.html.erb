
<table>
  <thead>
  <tr>
    <th>Amount</th>
    <th>Date done</th>
    <th>Group</th>
    <th>Sanction</th>
    <th>User</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% group_sanctions.each do |group_sanction| %>
    <tr>
      <td><%= group_sanction.amount %></td>
      <td><%= group_sanction.date_done %></td>
      <td><%= group_sanction.group_id %></td>
      <td><%= group_sanction.sanction_id %></td>
      <td><%= group_sanction.user_id %></td>
      <td><%= link_to 'Show', group_sanction %></td>
      <td><%= link_to 'Edit', edit_group_sanction_path(group_sanction) %></td>
      <td><%= link_to 'Destroy', group_sanction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>